    "##### Function for climatology and anomalies"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b0c43693-61d7-425b-ab89-d4c65c1c57e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "def climo(ds):\n",
    "    ds_climo = ds.groupby('time.month').mean(dim='time')\n",
    "    return ds_climo\n",
    "def anoms(ds):\n",
    "    ds_climo= ds.groupby('time.month').mean(dim='time')\n",
    "    ds_anoms = ds.groupby('time.month')-ds_climo\n",
    "    return ds_anoms"
   ]
  }
